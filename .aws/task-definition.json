{
   "containerDefinitions": [ 
      {
         "name": "qs-test",
         "image": "253236200379.dkr.ecr.eu-west-2.amazonaws.com/qs-assessment:latest",
         "cpu": 512,
         "memory": 1024,
         "memoryReservation": 50,
         "portMappings": [
             {
                 "name": "qs-test",
                 "containerPort": 3000,
                 "hostPort": 3000,
                 "protocol": "tcp"
             }
         ],
         "essential": true,
         "environment": [],
         "mountPoints": [],
         "volumesFrom": [],
         "linuxParameters": {
             "initProcessEnabled": true
         },
         "startTimeout": 30,
         "stopTimeout": 120,
         "user": "0",
         "privileged": false,
         "readonlyRootFilesystem": false,
         "interactive": false,
         "pseudoTerminal": false
     }
   ],
   "cpu": "1024",
   "taskRoleArn": "arn:aws:iam::253236200379:role/ecs_task_role",
   "executionRoleArn": "arn:aws:iam::253236200379:role/ecs_execution_role",
   "family": "fargate-task-definition",
   "memory": "2048",
   "networkMode": "awsvpc",
   "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    },
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}